{
  "expo": {
    "name": "BytApp",
    "slug": "BytApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ezehmark.BytApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ezehmark.BytApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "426c636f-b7ae-401b-9b3d-af6364917788"
      }
    },
    "owner": "ezehmark",

    "plugins": [
      [
        "expo-splash-screen",
        {
          "preventAutoHide": true
        }
      ]
    ]
  }
}
